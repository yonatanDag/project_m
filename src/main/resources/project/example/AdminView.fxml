<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.example.Controller.AdminController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../myTech.png" />
         </image>
      </ImageView>
      <Label text="When you press the button the Algorithm is starting">
         <font>
            <Font name="Bodoni MT Bold Italic" size="18.0" />
         </font>
      </Label>
      <Button fx:id="Btn1" minHeight="40.0" minWidth="40.0" mnemonicParsing="false" onAction="#clickBtn" style="-fx-background-radius: 100em;" text=" Click here ">
         <font>
            <Font name="Bodoni MT" size="12.0" />
         </font>
      </Button>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../myTech.png" />
         </image>
      </ImageView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
